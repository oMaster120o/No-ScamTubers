{
  "author": "Master120",
  "name": "N-STExten",
  "manifest_version": 3,
  "version": "1.0",
  "content_scripts": [{
    "matches": "<all_urls>",
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "<all_urls>",
    "nativeMessaging",
    "tabs"
  ],
}
